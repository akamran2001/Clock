{
  "manifest_version": 2,

  "name": "Today Extension",
  "description": "This extension is a new homepage for Chrome",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo 192.png",
    "48": "logo 512.png",
    "128": "logo 512.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxr...'; object-src 'self'",
  "permissions": ["management"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
